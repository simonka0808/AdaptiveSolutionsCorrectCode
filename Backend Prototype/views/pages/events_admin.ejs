<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head') %>
    <title>Sessions</title>
    <link rel="stylesheet" href="/css/events_page_admin.css">
    <link rel="stylesheet" href="/css/nav.css">

</head>


<div class="container-navbar">
    <%- include('../partials/nav_admin') %>
</div>



<body>

    <div class="maincontent">

            <% events.forEach(function (event) { %>
            <div class="container single-event-container container" style="border-style: solid; border-color: red;">
                <div class="row" style="background-color: red;color: white; padding: 15px;">
                    <div class="col">
                        <h2></h2>
                    </div>
                    <div class="col">
                        <h2><%= event.date_of_event %></h2>
                    </div>
                </div>
                <div class="row" style="padding: 15px;">
                    <div class="col">
                        <h1><%= event.location %></h1>
                    </div>
                    <div class="col">
                        <button class="delete-button" type="button"><h5>Delete Session</h5></button>
                        <button class="addmanual-button" type="button"><h5>Add User Manually</h5></button>
                    </div>
                </div>
                <div class="row" style="padding: 15px;">
                    <div class="col">
                        <h2><%= event.session_start_time+" "+event.session_end_time %></h2>
                    </div>
                    <div class="col">
                        <button class="attend-button" type="button"><h4>Edit Session</h4></button>
                        <button class="qrcode-button" type="button"><h4>QR CODE</h4></button>
                    </div>
                </div>
            </div>











<div class="neweventcontainer">
<div class="container">
    <div class="row">
      <div class="col-10">
        
      </div>
      <div class="col-2">
        <img src="images/andnewuser.png" alt="and new user button" style="max-width: 80%;height: auto;  ">
      </div>
  </div>
</div>
</body>